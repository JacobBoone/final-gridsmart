extends layout

block content
	h1 Gridsmart
	div.main-panel
		ul.nav.nav-tabs(role="tablist")
			li
				a(href="/") Property
			li
				a(href="/bills") Bills
			//- electric water and fuel
			li.active
				a(href="/footprint"  class="active") Usage Impact
		div.tab-content
			div#bills.tab-pane.active
				ul.nav.nav-pills.nav-justified(role="tablist")
					li
						a(href="/footprint") Energy Consumption
					li.active
						a(href="/waterfootprint"  class="active") Water Consumption
				div.tab-content
					div#footprint.tab-pane.active
						h3 Water Usage
						p Compares water usage year over year
						//- div#newchart(style="height:250px")
						div#waterchart(style="height:250px")
						table.table.my-tables
							tbody
								tr.my-row.my-tablerow.clearfix.hidden-xs
									th Jan
									th Feb
									th Mar
									th Apr
									th May
									th Jun
									th Jul
									th Aug
									th Sept
									th Oct
									th Nov
									th Dec




