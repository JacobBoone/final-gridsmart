extends layout

block content
	h1 Gridsmart
	div.main-panel
		ul.nav.nav-tabs(role="tablist")
			li
				a(href="/") Property
			li
				a(href="/bills") Bills
			//- electric water and fuel
			li.active
				a(href="/footprint"  class="active") Usage Impact
		div.tab-content
			div#bills.tab-pane.active
				ul.nav.nav-pills.nav-justified(role="tablist")
					li.active
						a(href="/footprint" class="active") Energy Consumption
					li
						a(href="/waterfootprint") Water Consumption
				div.tab-content
					div#footprint.tab-pane.active
						h3 Electric Usage
						p Compares electric usage year over year
						//- div#newchart(style="height:250px")
						div#electricchart(style="height:250px")
						//- div.morris-hover.morris-default-style(style="left: 0px; top: 173px;")
							div.morris-hover-row-label 2006
							div.morris-hover-point(style="color: #0b62a4") Series A: 100
							div.morris-hover-point(style="color: #7a92a3") Series B: 90
						table.table.my-tables
							tbody
								tr.my-row.my-tablerow.clearfix.hidden-xs
									th Jan
									th Feb
									th Mar
									th Apr
									th May
									th Jun
									th Jul
									th Aug
									th Sept
									th Oct
									th Nov
									th Dec



