extends layout

block content
	h1 Gridsmart
	div.main-panel
		ul.nav.nav-tabs(role="tablist")
			li
				a(href="/users/#{userId}") Property
			li
				a(href="/users/#{userId}/bills") Bills
			//- electric water and fuel
			li.active
				a(href="/users/#{userId}/footprint"  class="active") Carbon Footprint/ Usage
		div.tab-content
			div#bills.tab-pane.active
				ul.nav.nav-pills.nav-justified(role="tablist")
					li.active
						a(href="/users/#{userId}/footprint" class="active") Energy Consumption
					li
						a(href="/users/#{userId}/waterfootprint") Water Consumption
				div.tab-content
					div#footprint.tab-pane.active
						h3 Electric Usage
						//- div#newchart(style="height:250px")
						div#electricchart(style="height:250px")



